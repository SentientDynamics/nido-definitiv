{
	"name": "pl_A3_Payslips_DraftStagingToDraft",
	"properties": {
		"description": "Copy tnk_001.dbo.* to tnk_001.draft",
		"activities": [
			{
				"name": "df_A3_Payslips_DraftStagingToDraft",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "SetPipelineRunId",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "SetPipelineTriggerTime",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "df_A3_Payslips_DraftStagingToDraft",
						"type": "DataFlowReference",
						"parameters": {
							"PipelineTriggerTime": {
								"value": "'@{variables('PipelineTriggerTime')}'",
								"type": "Expression"
							},
							"PipelineRunId": {
								"value": "'@{variables('PipelineRunId')}'",
								"type": "Expression"
							},
							"PipelineName": {
								"value": "'@{pipeline().Pipeline}'",
								"type": "Expression"
							}
						},
						"datasetParameters": {
							"SourceSqlDraftStagingPreceda": {
								"SchemaName": "draft",
								"TableName": "StagingPrecedaPayslips"
							},
							"SinkSqlDraftPreceda": {
								"SchemaName": "draft",
								"TableName": "Preceda_Payslip_2"
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			},
			{
				"name": "SetPipelineTriggerTime",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "PipelineTriggerTime",
					"value": {
						"value": "@convertFromUtc(\n    coalesce(\n        pipeline().parameters.MasterPipelineTriggerTime,\n        pipeline().TriggerTime\n    ),\n    'AUS Eastern Standard Time'\n)",
						"type": "Expression"
					}
				}
			},
			{
				"name": "SetPipelineRunId",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "PipelineRunId",
					"value": {
						"value": "@coalesce(pipeline().parameters.MasterPipelineRunId,pipeline().RunId)",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"sourceSqlSchema": {
				"type": "string",
				"defaultValue": "draft"
			},
			"sourceSqlTable": {
				"type": "string",
				"defaultValue": "StagingPrecedaPayslips"
			},
			"sinkSqlSchema": {
				"type": "string",
				"defaultValue": "draft"
			},
			"sinkSqlTable": {
				"type": "string",
				"defaultValue": "Preceda_Payslip_2"
			},
			"MasterPipelineTriggerTime": {
				"type": "string"
			},
			"MasterPipelineRunId": {
				"type": "string"
			}
		},
		"variables": {
			"PipelineTriggerTime": {
				"type": "String"
			},
			"PipelineRunId": {
				"type": "String"
			}
		},
		"folder": {
			"name": "01_Payslips"
		},
		"annotations": [],
		"lastPublishTime": "2022-01-19T01:04:06Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}